    <queryString>
        <![CDATA[SELECT
tenant.tenant_id,
tenant.name,
msg.return_code,
count(1) total_rows,
max(msg.processing_time_in_millisecs) max_time,
round(avg(msg.processing_time_in_millisecs),2) avg_time
FROM p28_int_message msg
LEFT OUTER JOIN p42_cfg_tenants tenant on (msg.tenant_ref = tenant.object_ref)
WHERE
msg.execution_started_at >= to_timestamp($P{dateFromStr},'YYYY-MM-DD HH24:MI:SS')
AND msg.execution_started_at < to_timestamp($P{dateToStr},'YYYY-MM-DD HH24:MI:SS')
AND msg.return_code > 99999999
GROUP BY tenant.tenant_id, tenant.name, msg.return_code
ORDER BY  tenant_id, return_code, avg_time]]>
    </queryString>
