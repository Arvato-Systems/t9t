    <queryString>
        <![CDATA[SELECT
request_parameter_pqon,
count(1) total_rows,
round(avg(msg.processing_time_in_millisecs),0) avg_time,
max(msg.processing_time_in_millisecs) max_time,
min(msg.processing_time_in_millisecs) min_time
FROM p28_int_message msg
LEFT OUTER JOIN p42_cfg_tenants tenant on (msg.tenant_ref = tenant.object_ref)
WHERE
msg.processing_time_in_millisecs IS NOT NULL
AND msg.execution_started_at >= to_timestamp($P{dateFromStr},'YYYY-MM-DD HH24:MI:SS')
AND msg.execution_started_at < to_timestamp($P{dateToStr},'YYYY-MM-DD HH24:MI:SS')
GROUP by request_parameter_pqon
ORDER BY  request_parameter_pqon]]>
    </queryString>
