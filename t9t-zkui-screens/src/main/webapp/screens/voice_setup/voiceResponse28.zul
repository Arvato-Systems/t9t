                    <cells28  id="isSsml"             value="@load(vm.data.isSsml)           @save(vm.data.isSsml,before='commandSave')" />
                    <cells28  id="text"               value="@load(vm.data.text)             @save(vm.data.text,before='commandSave')" colspan1="3" rows1="6"/>
                </rows>
            </form28>
        </crud28>
    </threesections28>
</window28>
